{{#each this}}
{{#if dots}} <li class="pagination__item pagination__item--dots">{{page}}</li>
{{else if current}} 
<li class="pagination__item pagination__item--current">{{page}}</li>
{{else}} 
<li class="pagination__item"><a href="#" class="pagination__link pagination__link--page">{{page}}</a></li> 
{{/if}}
{{/each}}
